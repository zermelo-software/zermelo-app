<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="20180918" id="nl.zermelo.online.App" ios-CFBundleVersion="20180918" packageName="6D7654C9.ZermeloRooster" version="1.5.5" xmlns="http://www.w3.org/ns/widgets">
    <name>Zermelo</name>
    <description>Toont het schoolrooster en mededelingen van de school.</description>
    <author email="info@zermelo.nl" href="www.zermelo.nl">Zermelo Software B.V.</author>
    <access origin="https://*.zportal.nl/*" />
    <plugin name="cordova-plugin-nativestorage" spec="2.3.2" />
    <plugin name="cordova-plugin-splashscreen" spec="5.0.2" />
    <plugin name="cordova-plugin-statusbar" spec="2.4.2" />
    <plugin name="phonegap-plugin-barcodescanner" spec="8.0.0">
        <variable name="ANDROID_SUPPORT_V4_VERSION" value="27.+" />
        <variable name="CAMERA_USAGE_DESCRIPTION" value="De camera wordt gebruikt om QR-codes te scannen" />
    </plugin>
    <plugin name="cordova-plugin-whitelist" spec="1.3.3" />
    <preference name="orientation" value="portrait" />
    <preference name="autorotate" readonly="true" value="false" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="false" />
    <preference name="prerendered-icon" value="false" />
    <preference name="stay-in-webview" value="true" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="EnableViewportScale" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="TopActivityIndicator" value="gray" />
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />
    <preference name="HideKeyboardFormAccessoryBar" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="android-installLocation" value="internalOnly" />
    <preference name="ErrorUrl" value="" />
    <preference name="BackgroundColor" value="0xFFFFFFFF" />
    <preference name="KeepRunning" value="true" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="LoadUrlTimeoutValue" value="20000" />
    <preference name="disable-cursor" value="false" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="ShowSplashScreen" value="false" />
    <preference name="SplashScreenDelay" value="10000" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="19" />
        <preference name="android-targetSdkVersion" value="27" />
        <splash density="port-mdpi" src="www/resources/screens/screen-iphone-portrait.png" />
        <splash density="port-hdpi" src="www/resources/screens/SplashScreenPhone.scale-100.png" />
        <splash density="port-xhdpi" src="www/resources/screens/screen-iphone-portrait-667h.png" />
        <icon density="ldpi" src="www/resources/icons/36.png" />
        <icon density="mdpi" src="www/resources/icons/48.png" />
        <icon density="hdpi" src="www/resources/icons/72.png" />
        <icon density="xhdpi" src="www/resources/icons/96.png" />
        <icon density="xxhdpi" src="www/resources/icons/144.png" />
        <icon density="xxxhdpi" src="www/resources/icons/192.png" />
        <plugin name="cordova-plugin-backbutton" source="npm" spec="~0" />
        <engine name="android" spec="7.1.1" />
    </platform>
    <platform name="ios">
        <feature name="StatusBar">
            <param name="ios-package" onload="true" value="CDVStatusBar" />
        </feature>
        <preference name="StatusBarStyle" value="lightcontent" />
        <preference name="StatusBarBackgroundColor" value="#000000" />
        <preference name="BackupWebStorage" value="none" />
        <splash height="480" src="www/resources/screens/screen-iphone-portrait.png" width="320" />
        <splash height="960" src="www/resources/screens/screen-iphone-portrait-2x.png" width="640" />
        <splash height="1024" src="www/resources/screens/screen-ipad-portrait.png" width="768" />
        <splash height="2048" src="www/resources/screens/screen-ipad-portrait-2x.png" width="1536" />
        <splash height="768" src="www/resources/screens/screen-ipad-landscape.png" width="1024" />
        <splash height="1536" src="www/resources/screens/screen-ipad-landscape-2x.png" width="2048" />
        <splash height="1136" src="www/resources/screens/screen-iphone-portrait-568h-2x.png" width="640" />
        <splash height="1334" src="www/resources/screens/screen-iphone-portrait-667h.png" width="750" />
        <splash height="2208" src="www/resources/screens/screen-iphone-portrait-736h.png" width="1242" />
        <icon height="29" src="www/resources/icons/29.png" width="29" />
        <icon height="40" src="www/resources/icons/40.png" width="40" />
        <icon height="50" src="www/resources/icons/50.png" width="50" />
        <icon height="57" src="www/resources/icons/57.png" width="57" />
        <icon height="58" src="www/resources/icons/58.png" width="58" />
        <icon height="72" src="www/resources/icons/72.png" width="72" />
        <icon height="76" src="www/resources/icons/76.png" width="76" />
        <icon height="80" src="www/resources/icons/80.png" width="80" />
        <icon height="100" src="www/resources/icons/100.png" width="100" />
        <icon height="114" src="www/resources/icons/114.png" width="114" />
        <icon height="120" src="www/resources/icons/120.png" width="120" />
        <icon height="144" src="www/resources/icons/144.png" width="144" />
        <icon height="152" src="www/resources/icons/152.png" width="152" />
        <icon height="1024" src="www/resources/icons/1024.png" width="1024" />
        <plugin name="cordova-plugin-camera" spec="https://github.com/apache/cordova-plugin-camera">
            <variable name="CAMERA_USAGE_DESCRIPTION" value="De camera wordt gebruikt om QR-codes te scannen" />
        </plugin>
    </platform>
    <splash src="www/resources/screens/screen-iphone-portrait.png" />
    <icon src="www/resources/icons/192.png" />
    <engine name="android" spec="^7.1.1" />
</widget>
